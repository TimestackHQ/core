{
  "name": "timestack",
  "version": "1.0.0",
  "scripts": {
    "start": "export $(cat ../.env | xargs) && expo start --dev-client --port 8091",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eas:ios": "export $(cat ../.env | xargs) && eas build --profile development-simulator --platform ios",
    "eas:ios:no-sim": "export $(cat ../.env | xargs) && eas build --profile development --platform ios",
    "eas:android": "export $(cat ../.env | xargs) && eas build --profile development-simulator --platform android",
    "eas:ios:production": "eas build --platform ios -e production --auto-submit"
